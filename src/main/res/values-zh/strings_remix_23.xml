  <string name="YogsBrain_Name_Objective">Yog之脑</string>
  <string name="YogsBrain_Desc">Yog-Dzewa的大脑，所有邪恶的根源。</string>
  <!--STRINGS OF YogsEye CLASS-->
  <string name="YogsEye_Name">Yog之眼</string>
  <string name="YogsEye_Gender">masculine</string>
  <string name="YogsEye_Name_Objective">Yog之眼</string>
  <string name="YogsEye_Desc">Yog-Dzewa的眼睛，你感觉它充满仇恨的目光刺穿了你的灵魂。</string>
  <!--STRINGS OF YogsHeart CLASS-->
