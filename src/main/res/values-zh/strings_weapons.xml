  <!--STRINGS OF ShortSword CLASS-->
  <string name="ShortSword_Name">短剑</string>
  <string name="ShortSword_Info">它确实十分短，只比匕首长那么几英寸。</string>
  <string name="ShortSword_ACReforge">重铸</string>
  <string name="ShortSword_Select">选择你要强化的武器</string>
