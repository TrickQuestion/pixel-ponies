  <!--STRINGS OF Sungrass CLASS-->
  <string name="Sungrass_Name">サングラス</string>
  <string name="Sungrass_Desc">サングラスは、治癒性の樹液を持つことで有名です。</string>
  <string name="Sungrass_Buff">ハーバルヒーリング</string>
