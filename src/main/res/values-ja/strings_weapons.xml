  <!--PACKAGE /weapon/melee-->
  <!--STRINGS OF MeleeWeapon CLASS-->
  <string name="MeleeWeapon_Info1a">この %1$s は %2$s ランク-%3$d の近接武器です。</string>
  <string name="MeleeWeapon_Info1b">普通の</string>
  <string name="MeleeWeapon_Info1c">強化された</string>
  <string name="MeleeWeapon_Info1d">劣化した</string>
  <string name="MeleeWeapon_Info2a">平均ダメージは %d ポイントです。</string>
  <string name="MeleeWeapon_Info2b">平均ダメージは %1$d ポイントで、%2$d の力が必要です。</string>
  <string name="MeleeWeapon_Info2c">あなたにとって、この武器は重い。</string>
  <string name="MeleeWeapon_Info3a">これはかなり %s な武器です。</string>
  <string name="MeleeWeapon_Info3b">速い</string>
  <string name="MeleeWeapon_Info3c">遅い</string>
  <string name="MeleeWeapon_Info3d">と</string>
  <string name="MeleeWeapon_Info3e">だが</string>
  <string name="MeleeWeapon_Info3f">正確</string>
  <string name="MeleeWeapon_Info3g">不正確</string>
  <string name="MeleeWeapon_Info4a">これは速さを重視して作られています。</string>
  <string name="MeleeWeapon_Info4b">これは正確さを重視して作られています。</string>
  <string name="MeleeWeapon_Info5">これは魔術がかかっている。</string>
  <string name="MeleeWeapon_Info6a">あなたの力が不足しているため、この %s の正確さと攻撃速度が低下します。</string>
  <string name="MeleeWeapon_Info6b">あなたの力が十分なので、この %s のダメージが増加します。</string>
  <string name="MeleeWeapon_Info7a">あなたはすぐ使える状態 %2$s で %1$s を持っている。</string>
  <string name="MeleeWeapon_Info7b">、それは呪われているため、あなたは手放すことが出来ない</string>
  <string name="MeleeWeapon_Info7c">あなたは %s に潜んでいる邪悪な魔法を感じることが出来ます。</string>
