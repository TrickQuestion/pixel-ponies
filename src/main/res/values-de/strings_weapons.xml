  <string name="MeleeWeapon_Info1a">%1$s: %2$s Nahkampfwaffe der Stufe %3$d</string>
  <string name="MeleeWeapon_Info1b">eine</string>
  <string name="MeleeWeapon_Info1c">eine verbesserte</string>
