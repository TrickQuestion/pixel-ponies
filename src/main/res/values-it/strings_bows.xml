  <!--STRINGS OF CommonArrow CLASS-->
  <string name="CommonArrow_Name">freccia</string>
  <string name="CommonArrow_Info">Comune freccia di legno con una punta di metallo.</string>
  <string name="CommonArrow_Gender">femminile</string>
  <!--STRINGS OF FireArrow CLASS-->
  <string name="FireArrow_Name">freccia di fuoco</string>
